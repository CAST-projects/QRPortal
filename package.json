{
  "name": "qrportal",
  "version": "0.3.22",
  "description": "CAST Quality Rules Portal",
  "engines": {
    "node": "8.9.1"
  },
  "main": "server.js",
  "scripts": {
    "start": "set NODE_ENV=production&& \"node_modules/.bin/nodemon\" server.js",
    "start--mac": "env NODE_ENV=production&& \"node_modules/.bin/nodemon\" server.js",
    "start--linux": "env NODE_ENV=production PORT=2280&& \"node_modules/.bin/nodemon\" server.js",
    "dev": "set NODE_ENV=development&& \"node_modules/.bin/nodemon\" server.js",
    "build": "node_modules/.bin/webpack-cli --mode development",
    "build--watch": "node_modules/.bin/webpack-cli --mode development --watch",
    "build--production": "node_modules/.bin/webpack-cli --mode production",
    "test": "node_modules/.bin/mocha --require ./qrp_WebApp/test/testHelper.js ./qrp_WebApp/test/**/*Spec.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bpm": "0.0.2",
    "cors": "^2.8.4",
    "ejs": "^2.5.9",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "node-static": "^0.7.10",
    "npm": "^5.8.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "^3.5.0",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "request": "^2.81.0",
    "tape": "^4.7.0",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grager/qrportal"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "react",
    "api",
    "rest",
    "qualtiy-rules"
  ],
  "license": "MIT"
}
